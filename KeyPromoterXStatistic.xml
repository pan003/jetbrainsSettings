<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Column Selection Mode">
        <Item>
          <IdeaActionID>EditorToggleColumnMode</IdeaActionID>
          <ShortCut>'⇧⌘8'</ShortCut>
          <Description>Column Selection Mode</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Declaration">
        <Item>
          <IdeaActionID>GotoDeclaration</IdeaActionID>
          <ShortCut>'⌘B' or '⌘Button1 Click' or 'Button2 Click'</ShortCut>
          <Description>Declaration</Description>
          <Count>70</Count>
        </Item>
      </Statistic>
      <Statistic Action="Find Usages">
        <Item>
          <IdeaActionID>FindUsages</IdeaActionID>
          <ShortCut>'⌥F7'</ShortCut>
          <Description>Find Usages</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Implementation(s)">
        <Item>
          <IdeaActionID>GotoImplementation</IdeaActionID>
          <ShortCut>'⌥⌘B' or '⌥⌘Button1 Click'</ShortCut>
          <Description>Implementation(s)</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Jump to Source">
        <Item>
          <IdeaActionID>EditSource</IdeaActionID>
          <ShortCut>'⌘↓' or 'F4'</ShortCut>
          <Description>Jump to Source</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Rename...">
        <Item>
          <IdeaActionID>RenameElement</IdeaActionID>
          <ShortCut>'⇧F6'</ShortCut>
          <Description>Rename...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Revert...">
        <Item>
          <IdeaActionID>ChangesView.Revert</IdeaActionID>
          <ShortCut>'⌥⌘Z'</ShortCut>
          <Description>Revert...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Show Diagram...">
        <Item>
          <IdeaActionID>ShowUmlDiagram</IdeaActionID>
          <ShortCut>'⌥⇧⌘U'</ShortCut>
          <Description>Show Diagram...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'⌥F12'</ShortCut>
          <Description>Terminal</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Toggle Line Breakpoint">
        <Item>
          <IdeaActionID>ToggleLineBreakpoint</IdeaActionID>
          <ShortCut>'⌘F8'</ShortCut>
          <Description>Toggle Line Breakpoint</Description>
          <Count>3</Count>
        </Item>
      </Statistic>
      <Statistic Action="Type Declaration">
        <Item>
          <IdeaActionID>GotoTypeDeclaration</IdeaActionID>
          <ShortCut>'⇧⌘B' or '⌃⇧B' or '⇧⌘Button1 Click'</ShortCut>
          <Description>Type Declaration</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Version Control">
        <Item>
          <IdeaActionID>ActivateVersionControlToolWindow</IdeaActionID>
          <ShortCut>'⌘9' or '⇧⌘9'</ShortCut>
          <Description>Version Control</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
      <Statistic Action="Vim Emulator">
        <Item>
          <IdeaActionID>VimPluginToggle</IdeaActionID>
          <ShortCut>'⌥⌘V'</ShortCut>
          <Description>Vim Emulator</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Zoom Out">
        <Item>
          <IdeaActionID>UML.ZoomOut</IdeaActionID>
          <ShortCut>'⌘-' or '⌘-'</ShortCut>
          <Description>Zoom Out</Description>
          <Count>4</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>